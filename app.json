{
  "expo": {
    "name": "namcu",
    "slug": "namcu",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/ic.png",
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./local/assets/ic.png",
          "color": "#ffffff",
          "sounds": [
            "./local/assets/sound/ZURQ2FE-notification.mkv"
          ]
        }
      ]
    ],
  "splash": {
    "image": "./assets/images/Splash_page.png",
    "resizeMode": "cover",
    "backgroundColor": "#ffffff"
  },
  "updates": {
    "fallbackToCacheTimeout": 0
  },
  "assetBundlePatterns": [
    "**/*"
  ],
  "ios": {
    "supportsTablet": true
  },
  "android": {
    "adaptiveIcon": {
      "foregroundImage": "./assets/icons/ic.png",
      "backgroundColor": "#FFFFFF"
    },
    "package": "com.namcu.namcu",
    "versionCode": 1
  },
  "web": {
    "favicon": "./assets/icons/favicon.png"
  }
}
}